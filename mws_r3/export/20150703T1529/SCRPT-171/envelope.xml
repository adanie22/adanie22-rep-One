        <ns1:body>write-output &quot;Starting&quot;
# Load Common functions
. .\\Logging.ps1
. .\\GlobalRepository.ps1
. .\\LaunchProcess.ps1
. .\\FilesUtility.ps1
$scriptPath = split-path -parent $MyInvocation.MyCommand.Definition
$scriptName = $MyInvocation.MyCommand.Name
ConfigureLogging $scriptPath $scriptName
get-appfiles &quot;ForefrontIdentityManager&quot;
start-process &quot;msiexec.exe&quot; &quot;/i $scriptPath\\sqlncli.msi /q&quot; -wait
# get variables
$domainname = (get-globalvariable &quot;Global\DomainFQDN&quot;).value
$domainNB = (get-globalvariable &quot;Global\DomainNetBIOS&quot;).value
$username = &quot;svc_fim&quot;
$password = (get-globalvariable(&quot;ServiceAccount\$username&quot;)).value
$dbServer = &quot;DBServer&quot;

# Launch process
$args = &quot;/q /i `&quot;$scriptPath\\FIM\\Synchronization Service\\Synchronization Service.msi`&quot; STORESERVER=$DBServer SQLDB=FIMSynchronization SERVICEACCOUNT=$username SERVICEPASSWORD=$password SERVICEDOMAIN=$domainNB GROUPADMINS=FIMSyncAdmins GROUPOPERATORS=FIMSyncOperators GROUPACCOUNTJOINERS=FIMSyncJoiners GROUPBROWSE=FIMSyncBrowse GROUPPASSWORDSET=FIMSyncPasswordSet FIREWALL_CONF=1   /L*v C:\\deploy\\fimsetup.txt&quot;
write-output $args
#LaunchProcessAsUser &quot;msiexec.exe&quot; $args &quot;devmws\\agilitydeploy&quot; &quot;M3sh@dmin!&quot;</ns1:body>
