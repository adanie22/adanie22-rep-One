        <ns1:body># Script: Import SSL certificates
# Author: Marina Krynina
#################################################################################################
try
{
    $scriptPath = $env:USERPROFILE
    . .\LoggingV2.ps1 $true $scriptPath &quot;Import-Certificates.ps1&quot;
    . .\Import-Certificates.ps1 $scriptPath
    exit 0
}
catch
{
    $ex = $_.Exception | format-list | Out-String
    log &quot;ERROR: Exception occurred `nException Message: $ex&quot;

    exit 1
} 
