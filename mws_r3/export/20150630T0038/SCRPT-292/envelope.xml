        <ns1:body># Script: InstallOWA2013
# Author: Marina Krynina
#################################################################################################
# \USER_PROFILE
#        \Install
#        \Config
#        \InstallMedia
#                \OWA2013SP1x64
#        \Logs

try
{
    . .\LoggingV2.ps1 $true $env:USERPROFILE &quot;Install-OWA2013.ps1&quot;
    . .\Install\Install-OWA2013.ps1
    exit 0
}
catch
{
    $ex = $_.Exception | format-list | Out-String
    log &quot;ERROR: Exception occurred `nException Message: $ex&quot;

    exit 1
} 
