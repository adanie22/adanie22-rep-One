        <ns1:body>try
{
    $scriptPath = $env:USERPROFILE
    . .\LoggingV2.ps1 $true $scriptPath &quot;ExecuteWindowsCluster.ps1&quot;
    . .\ExecuteWindowsCluster.ps1

    exit 0
}
