<doc>
    <!-- 
        Minimum memory is in MB
        Ensure that the total minimum memory is not greater than total avialable (minus OS reserved amount), otherwise memory will not be set.
    -->
    <SQLMemoryConfig>
            <Mem ClusterNetworkName="CNN-001" SQLInstanceName="MWSPCDEVICES01" Min="8000"> </Mem>
            <Mem ClusterNetworkName="CNN-002" SQLInstanceName="MWSVDS02" Min="10000"> </Mem>
            <Mem ClusterNetworkName="CNN-003" SQLInstanceName="MWSAPPSENSE03" Min="10000"> </Mem>
            <Mem ClusterNetworkName="CNN-004" SQLInstanceName="MWSMOBILITY04" Min="6000"> </Mem>
            <Mem ClusterNetworkName="CNN-005" SQLInstanceName="MWSSSRS05" Min="2000"> </Mem>
    <!-- Only required for very large implementations, 25K+ users.
            <Mem ClusterNetworkName="CNN-006" SQLInstanceName="MWSTDT06" Min="6000"> </Mem>
	-->
            <Mem ClusterNetworkName="CNN-007" SQLInstanceName="MWSSCOM07" Min="8000"> </Mem>
            <Mem ClusterNetworkName="CNN-019" SQLInstanceName="MWSFIM19" Min="1000"> </Mem>
            <Mem ClusterNetworkName="CNN-020" SQLInstanceName="MWSPCDEVICES20" Min="8000"> </Mem>
    </SQLMemoryConfig>
    <!-- 
        Percentage of Total Server Memory available to reserve for OS.
        Eg. total memory = 64GB
            10 % = 6GB
            6GB of server memory will be reserved for OS
    -->
    <OSMemoryConfig>
            <Mem PercentageReserved="10"></Mem>
    </OSMemoryConfig>
</doc>
